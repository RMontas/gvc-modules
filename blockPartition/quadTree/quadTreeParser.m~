function [imPart, bitCounter] = quadTreeParser(quadtreeBitsream, bitCounter, imGradNorm, bMin, bCurrent, imPart)
%QUADTREEPARSER Summary of this function goes here
%   Detailed explanation goes here

dims = size(imGradNorm);
b=1;
while bCurrent > bMin
    dimsBlocks(b,1) = floor(dims(1) / bCurrent); % y
    dimsBlocks(b,2) = floor(dims(2) / bCurrent); % x
    bCurrent = bCurrent / 2;
    b = b + 1;
end

for hb = 1:dimsBlocks(1,1)
    for wb = 1:dimsBlocks(1,2)
        if quadtreeBitsream(bitCounter)
            [imPart, bitCounter] = quadTreeParser(quadtreeBitsream, bitCounter, imGradNorm, bMin, bCurrent/2, imPart);
        else
        end
    end
end

end

